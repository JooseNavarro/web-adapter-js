{
    "transform": {
        "\\.ts$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
        "ts",
        "tsx",
        "js",
        "jsx",
        "json"
    ],
    "verbose": true,
    "testPathIgnorePatterns" : [
        "<rootDir>/example/",
        "<rootDir>/dist/"
    ],
    "testEnvironment": "node"
}
